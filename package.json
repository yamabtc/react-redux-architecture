{
  "name": "react-typescript",
  "version": "0.1.0",
  "private": true,
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "post-commit": "git update-index -g"
    }
  },
  "scripts": {
    "---------- HELPERS -------------------------------------------------------------------------------------": "",
    "generate": "node ./tools/generate.js",
    "---------- DEVELOPMENT ---------------------------------------------------------------------------------": "",
    "start": "cross-env CLIENT_ENV=development craco start",
    "prod": "cross-env CLIENT_ENV=production craco start",
    "---------- PRODUCTION ----------------------------------------------------------------------------------": "",
    "build": "cross-env CLIENT_ENV=production craco build",
    "---------- TESTING -------------------------------------------------------------------------------------": "",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "lint": "tslint --project . --config tslint.json",
    "lint:fix": "npm run lint -- --fix",
    "--------------------------------------------------------------------------------------------------------": ""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@types/jest": "24.0.18",
    "@types/node": "12.7.3",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "axios": "0.19.0",
    "connected-react-router": "6.5.2",
    "dayjs": "1.8.16",
    "history": "4.9.0",
    "lodash.groupby": "4.6.0",
    "node-sass": "4.12.0",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-redux": "7.1.1",
    "react-router-dom": "5.0.1",
    "react-scripts": "3.1.1",
    "redux": "4.0.4",
    "redux-devtools-extension": "2.13.8",
    "redux-freeze": "0.1.7",
    "redux-thunk": "2.3.0",
    "reselect": "4.0.0",
    "semantic-ui-css": "2.4.1",
    "semantic-ui-react": "0.88.0",
    "sjs-base-model": "1.9.0",
    "ts-optchain": "0.1.8",
    "typescript": "3.6.2",
    "uuid": "3.3.3"
  },
  "devDependencies": {
    "@craco/craco": "5.4.0",
    "@types/history": "4.7.3",
    "@types/react-redux": "7.1.2",
    "@types/react-router-dom": "4.3.5",
    "@types/uuid": "3.4.5",
    "cross-env": "5.2.1",
    "generate-template-files": "2.2.0",
    "husky": "3.0.5",
    "lint": "0.7.0",
    "pretty-quick": "1.11.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-eslint-rules": "5.4.0",
    "tslint-react": "4.0.0"
  }
}
